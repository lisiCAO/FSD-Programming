package june16th;

public class Q10 {
	import java.util.HashSet;
	import java.util.Set;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("10. Write a Java program to find duplicate values in an array of string values.");
		System.out.println();
        
		String[] originalArray = {"apple", "banana", "cherry", "apple", "banana", "peach"};

        System.out.println("Original Array:");
        for (String s : originalArray) {
            System.out.print(s + " ");
            }
        Set<String> duplicates = findDuplicates(originalArray);
        }
	public static Set<String> findDuplicates(String[] originalArray) {
		Set<String> set =  new HashSet<>();
		Set<String> duplicates =  new HashSet<>();
		
		for(String value: originalArray) {
			if(!set.add(value)) {
				duplicates.add(value);
			}
			return duplicates;
		}
	}


}
