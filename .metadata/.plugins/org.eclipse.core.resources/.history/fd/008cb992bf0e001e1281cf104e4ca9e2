package June14;

import java.util.Scanner;

public class MyProgram7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner kb = new Scanner(System.in);

		System.out.println("Please give me positive num for Fibonacchi? ");
		int num = kb.nextInt();
		while (num < 0) {
			System.out.println("Wrong input please re enter : ");
			num = kb.nextInt();
		}
		int a=0,b=1,c=0 ;
		// 0 , 1, 1, 2 , 3 , 5 , 8,..
		for(int i = 1; i <= num;i++) {
			System.out.print(c + " ");
			a = b;
			b = c;
			c = a + b;
		}
		
	}

}
